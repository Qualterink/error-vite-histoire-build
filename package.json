{
  "name": "test",
  "version": "1.1.1",
  "nodeVersion": "v16.14.2",
  "description": "Library for test shared components in vue3 projects written in TS",
  "main": "dist/test.umd.js",
  "browser": "dist/test.esm.js",
  "module": "dist/test.es.js",
  "unpkg": "dist/test.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "dist/test.es.js",
      "require": "dist/test.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "scripts": {
    "vite-tsc": "vue-tsc --emitDeclarationOnly",
    "dev": "vite",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "traceResolution": "tsc --traceResolution",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "rimraf ./dist && vite build",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.9",
    "click-outside-vue3": "^4.0.1",
    "floating-vue": "^2.0.0-beta.17",
    "mitt": "^3.0.0",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "autoprefixer": "^10.4.8",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-vue": "^2.0.2",
    "clean-css": "^5.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.3.0",
    "fs-extra": "^10.1.0",
    "histoire": "^0.8.4",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "postcss-nesting": "^10.1.10",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.7.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.4.0",
    "vue-i18n": "^9.1.10",
    "vue-tsc": "^0.39.2"
  }
}
